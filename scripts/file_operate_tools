function fm-pdf_grep
{
	local app=pdfgrep
	local app_flag=
	which $app > /dev/null
	if [ $? -ne 0 ] ; then echo ¨$app not exist!¨;return 1;fi;
	
	$app $app_flag $*

	return 0
}

function fm-pdf_crack_passwd
{
	local app=pdfcrack
	local app_flag=
	which $app
	if [ $? -ne 0 ];then echo ¨$app not exist!¨;return 1;fi;

	$app $app_flag $*
	return 0
}

